    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.13.0",
    "sequelize": "^6.37.6",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "mocha": "8.4.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "sinon": "^19.0.4"
  }
