{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "dev:test": "NODE_ENV=test nodemon src/server.js",
    "start": "node src/server.js",
    "test:unit": "NODE_ENV=test mocha tests/unit/**/*.test.js --exit",
    "test:integration": "NODE_ENV=test mocha --file tests/integration/setup.js tests/integration/**/*.test.js --exit",
    "test": "npm run test:unit && npm run test:integration",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:dev": "NODE_ENV=development sequelize db:migrate",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:dev": "NODE_ENV=development sequelize db:migrate:undo",
    "db:migrate:undo:test": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:seed:dev": "NODE_ENV=development sequelize db:seed:all",
    "db:seed:test": "NODE_ENV=test sequelize db:seed:all",
    "db:seed:prod": "NODE_ENV=production sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "db:seed:undo:dev": "NODE_ENV=development sequelize db:seed:undo:all",
    "db:seed:undo:test": "NODE_ENV=test sequelize db:seed:undo:all",
    "db:create": "sequelize db:create",
    "db:create:dev": "NODE_ENV=development sequelize db:create",
    "db:create:test": "NODE_ENV=test sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:drop:dev": "NODE_ENV=development sequelize db:drop",
    "db:drop:test": "NODE_ENV=test sequelize db:drop",
    "test:setup": "NODE_ENV=test sequelize db:drop && NODE_ENV=test sequelize db:create && NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all"
  },
  "keywords": [],
